@model IEnumerable<Clothes>

@{
    var ModelName = "Search Clothes";
    ViewData["Title"] = ModelName;
}

<h2>@ModelName</h2>
<hr />
<div class="row">
    @foreach (var clothes in Model)
    {
        <div class="column">
            @Html.Partial("ClothesSummary", clothes)
        </div>
    }
</div>


